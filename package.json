{
  "name": "baguni",
  "version": "1.3.1",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/redgoose-dev/baguni.git",
  "author": "redgoose <scripter@me.com>",
  "scripts": {
    "app:install": "bun run --bun resource/install.js",
    "app:uninstall": "bun run --bun resource/uninstall.js",
    "dev": "NODE_ENV=development concurrently \"bun run --bun server/main.js\" \"bunx --bun vite\"",
    "prebuild": "rm -rf ./dist",
    "build": "bun run --bun ./server/build.js && bunx --bun vite build",
    "preview": "bun run --bun dist/main.js",
    "preview:server": "bun run --bun server/main.js",
    "version-patch": "npm version patch",
    "build:test": "rm -rf ./dist/chunk && rm ./dist/main.js && bun run --bun ./server/build.js && bun run preview"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@types/bun": "^1.2.22",
    "bcryptjs": "^3.0.2",
    "image-size": "^1.2.1",
    "jsonwebtoken": "^9.0.2",
    "minimist": "^1.2.8",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@redgoose/json-editor": "^1.2.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "concurrently": "^9.2.1",
    "image-resize": "^1.4.1",
    "lucide-vue-next": "^0.542.0",
    "marked": "^16.3.0",
    "ofetch": "^1.4.1",
    "pinia": "^3.0.3",
    "sass": "^1.92.1",
    "terser": "^5.44.0",
    "vite": "^6.3.6",
    "vue": "^3.5.21",
    "vue-advanced-cropper": "^2.8.9",
    "vue-router": "^4.5.1"
  }
}
